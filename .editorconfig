root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = false

[*.py]
trim_trailing_whitespace = true
max_line_length = 120

[*.{js,jsx,json,scss,html}]
indent_size = 2
trim_trailing_whitespace = true
max_line_length = 120
